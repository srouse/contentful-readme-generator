{
  "name": "contentful-readme-generator",
  "version": "0.0.2",
  "description": "Generate a README.md file from content stored within https://www.contentful.com",
  "main": "index.js",
  "author": "srouse",
  "license": "MIT",
  "bin": {
    "ctfl-readme": "./bin/cli.mjs"
  },
  "scripts": {
    "build": "ctfl-readme build",
    "test:build": "ctfl-readme build --fileName README-TESTING.md",
    "test:help": "ctfl-readme --help"
  },
  "dependencies": {
    "badges": "^4.27.0",
    "chalk": "^5.0.1",
    "contentful": "^9.1.34",
    "dotenv": "^16.0.1",
    "markdown-toc": "^1.2.0",
    "yargs": "^17.5.1"
  }
}
